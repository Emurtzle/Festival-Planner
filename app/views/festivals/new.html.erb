<h1>Create a Festival!</h1>
<%= form_for @festival do |f|%>
<%= f.label :name %>
<%= f.text_field :name %><br>

<%= f.label :city %>
<%= f.text_field :city %><br>

<%= f.label :description %>
<%= f.text_field :description %><br>

<%= f.label :duration %>
<%= f.number_field :duration %> Days <br>

<%= f.submit "Create Your Festival"%>
<% end %>
